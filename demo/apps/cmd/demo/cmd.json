	"state": 1,
	// 脚本路径
	"script": "./cmd.js",
	"cmd": [{
			"name": "示例指令"
			// 如果没有写明指令规则，则是以“#[指令名称] ”为前缀内容触发指令
		},
		{
			"name": "正则匹配",
			// 用正则表达式来判断用户输入内容是否触发指令
			"rule": "/^#正则匹配/"
		},
		{
			"name": "前缀匹配",
			// 用用户输入内容是否以某关键词前缀决定是否触发指令
			"rule": "前缀匹配*"
		},
		{
			"name": "后缀匹配",
			// 用用户输入内容是否以某关键词作为后缀决定是否触发指令
			"rule": "*后缀匹配"
		},
		{
			"name": "包含匹配",
			// 用用户输入内容是否包含关键词决定是否触发指令
			"rule": "*包含匹配*"
		},
